{
  "name": "ai-image-editor-functions",
  "version": "1.0.0",
  "description": "Firebase Functions for AI Image Editor",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
