    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "busboy": "^1.6.0",
    "cors": "^2.8.5",
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^5.0.0",
    "multer": "^1.4.5-lts.1",
    "stripe": "^18.5.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.12",
    "typescript": "^5.1.0"
  }
}